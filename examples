#Dirichlet bc example
h = 0.1
V = [[0, 0.81], [0.15, 0.82], [0.23, 0.78], [0.31, 0.81]]
E = [[0,1], [2, 3]]
bc = [['t',1,0], ['t',1,0],['t',1,0],['t',1,0]]
S = FD_solve(V,E,bc,h)

#Neumann bc example
h = 0.1
V = [[0, 0.81], [0.15, 0.82], [0.23, 0.78], [0.31, 0.81]]
E = [[0,1], [2, 3]]
bc = [['t','-',1], ['t','-',1],['t','-',1],['t','-',1]]
S = FD_solve(V,E,bc,h)
